{
  "compilerOptions": {
    "target": "ES5",
    "module": "Preserve",
    "noLib": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": false,
    "outDir": "dist",
    "typeRoots": [
      "node_modules/@wshcmx/types/lib",
      "node_modules/@wshcmx/types/lib/xml"
    ]
  },
  "include": [
    "src/**/*"
  ],
  "files": [
    "lib/lib.ts"
  ],
  "exclude": [
    "src/exclude_submodule.ts"
  ]
}
